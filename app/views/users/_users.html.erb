<% if users.any? %>
  <ul class="list-unstyled">
    <% users.each do |user| %>
      <li class="media">
        <div class="media-body">
          <div>
            <p><%= link_to user.name, user_path(user) %></p>
          </div>
          <div> 
            <%= render 'relationships/follow_button', user: user %>
          </div>
        </div>
      </li>
    <% end %>
  </ul>
  <%= paginate users %>
<% end %>